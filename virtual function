
#include<iostream>
#include<string>
using namespace std;
class Parent{
    public:
    virtual void hello(){
        cout<<"Hello from parent!"<<endl;
    }
};
class Child: public Parent{
    public:
    void hello(){
        cout<<"Hello from child!"<<endl;
    }
};
int main(){
    Parent p1;
    p1.hello();
    Child c1;
    c1.hello();
    return 0;
}
